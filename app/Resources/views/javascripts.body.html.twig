{% block bodyscripts %}

    {% javascripts output="js/organic-calendar.js"
        "%kernel.root_dir%/../vendor/arshaw/fullcalendar/dist/fullcalendar.min.js"
        "@CaravaneCalendarBundle/Resources/public/js/calendar-settings.js"
    %}
        <script type="text/javascript" src="{{ asset_url }}"></script>
    {% endjavascripts %}


    {% javascripts output="js/organic-ui.js"
        "%kernel.root_dir%/../vendor/biggora/bootstrap-ajax-typeahead/js/bootstrap-typeahead.js"
        "%kernel.root_dir%/../vendor/voidberg/html5sortable/dist/html.sortable.js"
        "%kernel.root_dir%/../vendor/dangrossman/bootstrap-daterangepicker/daterangepicker.js"
        "@CaravaneOrganicBundle/Resources/public/js/main.js"
    %}
        <script type="text/javascript" src="{{ asset_url }}"></script>
    {% endjavascripts %}

    {% javascripts filter='?yui_js' output="js/organic-bijui.js"
        "@CaravaneBIJUploadBundle/Resources/public/js/vendor/jquery.ui.widget.js"
        "@CaravaneBIJUploadBundle/Resources/public/js/vendor/jquery.iframe-transport.js"
        "@CaravaneBIJUploadBundle/Resources/public/js/vendor/jquery.fileupload.js"
        "@CaravaneBIJUploadBundle/Resources/public/js/main.js"

    %}
        <script type="text/javascript" src="{{ asset_url }}"></script>
    {% endjavascripts %}



{% endblock bodyscripts %}
