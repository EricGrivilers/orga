    /**
     * @var \Products
     *
     * @ORM\OneToMany(targetEntity="Product2offre",mappedBy="offreid")
     * @ORM\OrderBy({"rank"="asc"})
     */

    private $products;


     /**
     * @var \SLices
     *
     * @ORM\ManyToMany(targetEntity="Condition")
     */
