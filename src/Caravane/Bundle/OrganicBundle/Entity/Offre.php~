    /**
     * @var \DateTime
     *
     * @ORM\Column(name="updateDate", type="datetime", nullable=false)
     */
    private $updatedate;

    /**
     * @var \DateTime
     *
     * @ORM\Column(name="previewDate", type="datetime", nullable=true)
     */
    private $previewdate;

    /**
     * @var \User
     *
     * @ORM\ManyToOne(targetEntity="Caravane\UserBundle\Entity\User")
     */
