<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="ExtraPlanning2offre" table="extra_planning2offre">
    <id name="planningid" type="integer" column="planningId">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="planningtype" type="string" column="planningType" length="50" nullable="false"/>
    <field name="startdate" type="datetime" column="startDate" nullable="false"/>
    <field name="enddate" type="datetime" column="endDate" nullable="false"/>
    <field name="etat" type="string" column="etat" length="50" nullable="false"/>
    <many-to-one field="offreid" target-entity="ExtraOffres">
      <join-columns>
        <join-column name="offreId" referenced-column-name="offreId"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="jobid" target-entity="ExtraJobs">
      <join-columns>
        <join-column name="jobId" referenced-column-name="jobId"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="userid" target-entity="ExtraUsers">
      <join-columns>
        <join-column name="userId" referenced-column-name="userId"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
