<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="ExtraDatas" table="extra_datas">
    <id name="dataid" type="integer" column="dataId">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="parentid" type="integer" column="parentId" nullable="false"/>
    <field name="rank" type="integer" column="rank" nullable="false"/>
    <field name="subrank" type="integer" column="subRank" nullable="false"/>
    <field name="link" type="string" column="link" length="255" nullable="false"/>
    <field name="url" type="string" column="url" length="255" nullable="false"/>
    <field name="thumburl" type="string" column="thumbUrl" length="255" nullable="false"/>
    <field name="title" type="string" column="title" length="120" nullable="false"/>
    <field name="shortdescription" type="string" column="shortDescription" length="255" nullable="false"/>
    <field name="longdescription" type="text" column="longDescription" nullable="false"/>
    <field name="info1" type="text" column="info1" nullable="false"/>
    <field name="info2" type="text" column="info2" nullable="false"/>
    <field name="info3" type="text" column="info3" nullable="false"/>
    <field name="categoryid" type="integer" column="categoryId" nullable="false"/>
    <field name="insertdate" type="datetime" column="insertDate" nullable="false"/>
  </entity>
</doctrine-mapping>
