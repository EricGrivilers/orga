<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Document" table="document">
    <id name="documentid" type="integer" column="documentId">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="linkto" type="string" column="linkTo" length="20" nullable="false"/>
    <field name="linkid" type="integer" column="linkId" nullable="false"/>
    <field name="type" type="string" column="type" length="7" nullable="false"/>
    <field name="url" type="string" column="url" length="255" nullable="false"/>
  </entity>
</doctrine-mapping>
