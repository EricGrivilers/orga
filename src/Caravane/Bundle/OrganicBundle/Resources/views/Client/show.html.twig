        <tr>
            <th>Origin</th>
            <td>{{ entity.origin }}</td>
        </tr>
        <tr>
            <th>Insertdate</th>
            <td>{{ entity.insertdate|date('Y-m-d H:i:s') }}</td>
        </tr>
        <tr>
            <th>Updatedate</th>
            <td>{{ entity.updatedate|date('Y-m-d H:i:s') }}</td>
        </tr>
        <tr>
            <th>Public</th>
            <td>{{ entity.public }}</td>
        </tr>
    </tbody>
</table>

<ul class="record_actions">
    <li>
        <a href="{{ path('client') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('client_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>
        <form action="{{ path('client_delete', { 'id': entity.id }) }}" method="post">
            {{ form_widget(delete_form) }}
            <button type="submit">Delete</button>
        </form>
    </li>
</ul>
