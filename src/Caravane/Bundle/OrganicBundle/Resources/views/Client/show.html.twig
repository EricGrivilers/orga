        <tr>
            <th>Cietype</th>
            <td>{{ entity.cietype }}</td>
        </tr>
        <tr>
            <th>Vat</th>
            <td>{{ entity.vat }}</td>
        </tr>
        <tr>
            <th>Address</th>
            <td>{{ entity.address }}</td>
        </tr>
        <tr>
            <th>Number</th>
            <td>{{ entity.number }}</td>
        </tr>
        <tr>
            <th>Zip</th>
            <td>{{ entity.zip }}</td>
        </tr>
        <tr>
            <th>City</th>
            <td>{{ entity.city }}</td>
        </tr>
        <tr>
            <th>Country</th>
            <td>{{ entity.country }}</td>
        </tr>
        <tr>
            <th>Phone</th>
            <td>{{ entity.phone }}</td>
        </tr>
        <tr>
            <th>Phone2</th>
            <td>{{ entity.phone2 }}</td>
        </tr>
        <tr>
            <th>Mobile</th>
            <td>{{ entity.mobile }}</td>
        </tr>
        <tr>
            <th>Fax</th>
            <td>{{ entity.fax }}</td>
